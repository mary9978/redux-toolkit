rtSlice.js",[],[],"/Users/salmanmoghimi/Desktop/React-saheb/cart/src/component/Product.js",["110"],[],"import { useSelector, useDispatch } from \"react-redux\";\nimport { add, remove } from \"../redux/slice/cartSlice\";\nimport { useSnackbar } from \"notistack\";\nimport Cart from \"../pages/Cart/Cart\";\nimport { Link } from \"react-router-dom\";\nconst Product = ({item}) => {\n    const { cart } = useSelector((state) => state);\n    const dispatch = useDispatch();\n    const { enqueueSnackbar}  = useSnackbar();\n    const addtocartHandler = (item) => {\n        dispatch(add(item));\n        enqueueSnackbar(`Item added to your cart successfully`, {\n          variant: \"success\",\n          autoHideDuration: 3000,\n        });\n      };\n      const removeitemHandler = (id) => {\n        dispatch(remove(id));\n        enqueueSnackbar(`Item removed from your cart!`, {\n          variant: \"error\",\n          autoHideDuration: 3000,\n        });\n      };\n      console.log('product in cart',cart);\n  return (\n    <>\n      <h2>item in cart - {cart.length}</h2>\n      <Link to={\"/cart\"}>cart</Link>\n      <div>\n        <img src={item.image} alt={item.title} width={'200px'}/>\n        <h6>{item.title}</h6>\n        <p>${item.price}</p>\n        {cart.some((p) => p.id === item.id)?(\n          <button onClick={()=>removeitemHandler(item.id)}>Remove item</button>\n        ):(\n          <button onClick={()=>addtocartHandler(item)}>add item</button>\n        )}\n      </div>\n    </>\n  );\n};\n\nexport default Product;\n","/Users/salmanmoghimi/Desktop/React-saheb/cart/src/pages/Home/Home.js",[],[],"/Users/salmanmoghimi/Desktop/React-saheb/cart/src/pages/Cart/Cart.js",[],[],"/Users/salmanmoghimi/Desktop/React-saheb/cart/src/data.js",[],[],{"ruleId":"111","severity":1,"message":"112","line":4,"column":8,"nodeType":"113","messageId":"114","endLine":4,"endColumn":12},"no-unused-vars","'Cart' is defined but never used.","Identifier","unusedVar"]